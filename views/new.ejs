<%- include('partials/head.ejs') %>

<%- include('partials/nav.ejs') %>

<div style="padding:80px ;"  class="container">
    <hr>
 <div class="card">
    <div class="card-header">
     <h2>Cadastro de Usuario</h2>
    </div>
    <hr>
    <div class="card-body">
      <form method="post" action="/categories/save">
         <label>Nome:</label>
         <input class="form-control" type="text" name="nome" placeholder="Seu nome!">
         <br>
         <label>Sobrenome:</label>
         <input class="form-control" type="text" name="sobrenome" placeholder="Seu Sobrenome!">
         <br>
         <label>Email:</label>
         <input class="form-control" type="email" name="email" placeholder="Digite o seu E-mail!">
         <br>
         <label>Telefone:</label>
         <input class="form-control" type="number" name="telefone" placeholder="Digite o seu Telefone">
         <br>

         <button class="btn btn-success">Cadastrar</button>
       </form>
    </div>
 </div>
</div>



